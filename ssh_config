Host vm1
  Hostname 192.168.64.2
  User kg
  Port 22
  ControlMaster auto
  ControlPath ~/.ssh/sockets-%r@%h-%p
  ControlPersist yes
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking no

Host vm2
  Hostname localhost
  User kg
  Port 2222
  ControlMaster auto
  ControlPath ~/.ssh/sockets-%r@%h-%p
  ControlPersist yes
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking no


